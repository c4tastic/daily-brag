{
  "name": "@c4tastic/daily-brag",
  "private": false,
  "description": "A simple reminder to document your daily accomplishments",
  "version": "0.0.1",
  "author": "Lucien Bénié <lucien.benie@gmail.com>",
  "contributors": [
    "Lucien Bénié <lucien.benie@gmail.com>"
  ],
  "bin": {
    "daily-brag": "./bin/run"
  },
  "bugs": "https://github.com/git@github.com:c4tastic/daily-brag.git/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@octokit/core": "^3.2.4",
    "inquirer": "^7.3.3",
    "inquirer-file-tree-selection-prompt": "^1.0.7",
    "node-fetch": "^2.6.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/inquirer": "^7.3.1",
    "@types/mustache": "^4.1.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.17.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^11.0.2",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "mustache": "^4.1.0",
    "oclif": "^1.16.1",
    "os-locale": "^5.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "validate-github-token": "^1.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/git@github.com:c4tastic/daily-brag.git",
  "keywords": [
    "oclif",
    "daily",
    "brag",
    "daily brag"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "daily-brag",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "git@github.com:c4tastic/daily-brag.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint 'src/**/*.ts'"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --quiet"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
